{
  "name": "vuedi",
  "version": "0.1.0",
  "description": "A lightweight, type-safe Dependency Injection (DI) container for Vue 3 using the Composition API.",
  "keywords": [
    "vue",
    "vue3",
    "dependency-injection",
    "di",
    "ioc",
    "composition-api",
    "typescript",
    "vue-plugin"
  ],
  "homepage": "https://github.com/istiuak-0/vuedi#readme",
  "bugs": {
    "url": "https://github.com/istiuak-0/vuedi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istiuak-0/vuedi.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": {
    "name": "Istiuak",
    "url": "https://github.com/istiuak-0"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./router": {
      "types": "./dist/router.d.ts",
      "import": "./dist/router.js",
      "require": "./dist/router.cjs"
    },
    "./store": {
      "types": "./dist/store.d.ts",
      "import": "./dist/store.js",
      "require": "./dist/store.cjs"
    },
    "./query": {
      "types": "./dist/query.d.ts",
      "import": "./dist/query.js",
      "require": "./dist/query.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.16",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.18.0"
}